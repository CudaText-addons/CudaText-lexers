  TokenRules = <
    item
      DisplayName = 'parser-Comment'
      StyleName = 'Comment'
      TokenType = 1
      Expression = ';.*'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-StartRepeat'
      StyleName = 'Id keyword'
      TokenType = 2
      Expression = '(Begin\.[a-z_]+|End\.[a-z_]+|Repeat\.[a-z_]+)\b'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-StartIf'
      StyleName = 'Id keyword'
      TokenType = 2
      Expression = '(If|\.If)\b'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-EndIf'
      StyleName = 'Id keyword'
      TokenType = 2
      Expression = '(end\.If)\b'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-Hex'
      StyleName = 'Number'
      TokenType = 5
      Expression = '[\da-f]+h\b | [01]+b\b | 0x\h+\b'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-Number'
      StyleName = 'Number'
      TokenType = 5
      Expression = '\d+d?\b'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-BeginSub'
      StyleName = 'Label'
      Expression = '(?i)(?<=begin\.sub\x20).*$'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-BeginFunction'
      StyleName = 'Label'
      Expression = '(?i)(?<=begin\.function\x20).*$'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-Asm'
      StyleName = 'Std prefix'
      TokenType = 2
      Expression = '@\w+'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-Variables'
      StyleName = 'Id'
      TokenType = 2
      Expression = '[a-z_%]\w*'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-String'
      StyleName = 'String'
      TokenType = 4
      Expression = '("|'#39').*?(\1|$)'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'parser-Symbol'
      StyleName = 'Symbol'
      TokenType = 3
      Expression = '[!=\,:\-\+\*\\/%@<>\(\)\[\]\{\}\?]'
      ColumnFrom = 0
      ColumnTo = 0
    end>
